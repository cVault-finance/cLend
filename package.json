{
  "name": "clend",
  "version": "1.0.0",
  "description": "Contracts for the CORE DAO and Lending",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "hardhat clean",
    "build": "hardhat compile",
    "rebuild": "hardhat clean && hardhat compile",
    "accounts": "hardhat accounts",
    "start": "cross-env HARDHAT_NETWORK_LIVE=true hardhat node",
    "start:slowmining": "cross-env HARDHAT_NETWORK_LIVE=true SLOW_MINING=true hardhat node",
    "coverage": "hardhat coverage",
    "test": "hardhat test test/clending.test.js test/lending.test.ts test/coredao.test.ts test/governance.test.ts test/treasury.test.ts",
    "test:gas": "cross-env REPORT_GAS=true yarn test",
    "storage": "hardhat check",
    "watch": "hardhat watch compilation",
    "void:deploy": "cross-env HARDHAT_NETWORK_LIVE=true hardhat deploy",
    "mainnet:deploy": "hardhat --network mainnet deploy",
    "mainnet:verify": "hardhat --network mainnet etherscan-verify --solc-input",
    "mainnet:export": "hardhat --network mainnet export --export exports/mainnet.json",
    "goerli:deploy": "hardhat --network goerli deploy",
    "goerli:verify": "hardhat --network goerli etherscan-verify --solc-input",
    "goerli:export": "hardhat --network goerli export --export exports/goerli.json"
  },
  "author": "",
  "license": "UNLICENSED",
